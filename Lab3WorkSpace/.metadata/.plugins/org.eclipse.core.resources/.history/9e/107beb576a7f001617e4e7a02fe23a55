#ifndef SPRITE_H_
#define SPRITE_H_

#include <stdint.h>		//For integer types

typedef struct sprite_data_t
{
	uint16_t width,
	uint16_t height,
	uint32_t *sprite_data
};

#endif /* SPRITE_H_ */
